{
  "manifest_version": 2,
  "name": "YouTube Playlist Extension",
  "version": "1.0",
  "description": "Converts Youtube Playlist video into a list of videos",
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}
